@model List<Empleado>

<h1 style="color:green">Session Empleados</h1>
<p><a asp-asp-controller="Empleados" asp-action="EmpleadosAlmacenados">Empleados almacenados</a></p>

<ul class="list-group">
	@foreach (Empleado empleado in Model)
	{
		<li class="list-group-item">
			@empleado.Apellido
			<a asp-asp-controller="Empleados" asp-action="SessionEmpleados" asp-route-idEmpleado="@empleado.IdEmpleado">
				Almacenar empleado
			</a>
		</li>
	}
</ul>
